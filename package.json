  "main": "index.js",
  "scripts": {
    "test:hello": "echo 'Hello World'",
    "build:css": "./node_modules/.bin/tailwindcss -i ./src/input.css -o ./public/dist/output.css --minify",
    "build:ts": "tsc",
    "build:11ty": "eleventy",
    "build": "npm-run-all build:css build:ts build:11ty",
    "start": "npm-run-all --parallel watch:*",
    "watch:css": "./node_modules/.bin/tailwindcss -i ./src/input.css -o ./public/dist/output.css --watch",
    "watch:11ty": "eleventy --serve",
    "watch:ts": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:mobile": "playwright test --project=\"Mobile Chrome\"",
    "test:e2e:report": "playwright show-report",
    "test:all": "npm-run-all test test:e2e",
    "playwright:install": "playwright install",
    "playwright:install:deps": "playwright install-deps"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-img": "^6.0.4",
    "@playwright/test": "^1.55.1",
    "@quasibit/eleventy-plugin-sitemap": "^2.2.0",
    "@tailwindcss/typography": "^0.5.10",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.2",
    "html-minifier-terser": "^7.2.0",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "npm-run-all": "^4.1.5",
    "sharp": "^0.34.3",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.4.4",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.2"
  }
